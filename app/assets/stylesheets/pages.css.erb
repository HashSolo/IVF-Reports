/* Colors: Mid: #2F7C7C Mid-Dark:#325D5D Dark:#0F5151 Mid-Light:#65BDBD Light:#7DBDBD
/*background*/
body {background: url("<%= asset_path 'bg.png' %>") repeat-x #eeeeee;font-weight:normal;font-size:10px;font-family:Arial,Tahoma, Helvetica, sans-serif;color:#222222;font-size:15px;}

/*clinicians page*/
img.clinicians_page {display:inline-block;float:right;}

/*Home Page Call to Action*/
div.call_to_action_page {border-bottom:1px solid #cccccc;display:inline-block;width:98%;margin-right:2%;}
div.call_to_action_page h1 {text-align:left;font-weight:normal;font-family:'Trebuchet MS', Arial, sans-serif;color:#00345c;font-size:20px;margin-bottom:10px;}
div.call_to_action{border:1px solid #7895a3;margin-bottom:24px;background:#f5f5f5;display:inline-block;width:45%;float:left;}
div.call_to_action:last-child{margin-left:4%;}
div.call_to_action h2 {background:#7895a3;margin:0px;font-size:20px;color:#ffffff;padding-top:6px;padding-bottom:6px;padding-left:12px;}
div.call_to_action ul {list-style-type:square;color:#325d64;margin-left:30px;margin-top:24px;margin-bottom:24px;display:inline-block;width:100%;float:left;}
div.call_to_action ul li {font-size:14px;margin-bottom:12px;display:inline-block;width:100%;float:left;}
div.call_to_action ul li:last-child {margin-bottom:0px;}
div.call_to_action span.register_now {display:inline-block;height:30px;line-height:30px;text-align:center;margin-left:30%;margin-right:30%;width:40%;float:left;border:3px solid #ffffff;background:#7895a3;margin-bottom:24px;}
div.call_to_action span.register_now a {text-decoration:none;display:block;height:30px;text-align:center;line-height:30px;color:#ffffff;font-weight:bold;}
div.call_to_action span.register_now a:hover {background:#325d64;color:#cccccc;}

/*header styles*/
div.header_top {display:block;}
span.logo {background: url("<%= asset_path 'logo.png' %>") no-repeat center left;display:inline-block;height:100px;width:250px;float:left;}
span.logo a {display:block;height:100px;}
span.authentication {display:inline-block;width:600px;float:right;height:100px;text-align:right;}
span.authentication ul {display:inline-block;width:100%;margin:0px;padding:0px;float:right;}
span.authentication ul li {display:inline-block;width:20%;margin:0px;padding:0px;list-style-type:none;height:40px;margin-left:5px;text-align:center;line-height:40px;float:right;overflow:hidden;}
span.authentication ul li span.icon {line-height:50px;}
span.authentication ul li.divider {width:10px;}
span.authentication ul li a {text-decoration:none;color:#325d64;font-size:13px;}
span.authentication ul li a:hover {color:#7895a3;}
div.header_nav {display:block;}
nav.header ul {display:inline-block;width:100%;margin:0px;padding:0px;float:left;}
nav.header ul li {display:inline-block;width:16%;height:40px;line-height:40px;text-align:center;float:left;list-style-type:none;}
nav.header ul li.selected {margin-top:-3px;height:42px;background:#7895a3;-moz-border-radius-topright:10px;-moz-border-radius-topleft:10px;border-top-left-radius:10px;border-top-right-radius:10px;-webkit-border-top-left-radius:10px;-webkit-border-top-right-radius:10px;}
nav.header ul li a {text-decoration:none;color:#ffffff;display:block;height:40px;}
nav.header ul li a:hover {color:#cccccc;}
nav.header ul li.search {color:#ffffff;width:20%;text-align:right;float:right;font-size:13px;}
nav.header ul li.search:hover {color:#cccccc;}

/*header picture*/
.header_pic{margin-top:1px;background:url("<%= asset_path 'headerpic.png' %>") no-repeat;height:240px;}


/* Google Map Stuff */
div.google_map_canvas {border:2px solid #325d64;width:100%;height:350px;}
div.gmap_score_section ul.gmap {margin:0px;display:inline-block;float:left;width:100%;padding:0px;margin-bottom:20px;}
div.gmap_score_section ul.gmap li {list-style-type:none;margin:0px;padding:0px;display:inline-block;float:left;width:80%;height:25px;line-height:25px;border-bottom:1px solid #bbbbff;}
div.gmap_score_section ul.gmap li span.score{display:inline-block;float:right;}
div#zip_input {display:inline-block;width:46%;margin-left:27%;margin-right:27%;margin-bottom:0px;float:left;}
div.zip_explanation {text-align:center;margin-bottom:30px;font-size:12px;margin-top:10px;display:inline-block;float:left;width:100%;}


/*Ranking Page*/
div.ranking_tabs {border:0px solid #ffffff;display:inline-block;width:100%;}
div.ranking_tabs ul.jqueryui-ranking-tabs {width:431px;background:#E6E6E6 url(images/ui-bg_glass_75_e6e6e6_1x400.png) 50% 50% repeat-x;display:inline-block;}
div.ranking_tabs ul.jqueryui-ranking-tabs li.ranking_tab {height:30px;line-height:30px;text-align:center;width:140px;display:inline-block;}
div.ranking_tabs ul.jqueryui-ranking-tabs li.ranking_tab a {text-align:center;display:inline-block;width:100%;}

/*Ranking - Age Subselect*/
.jqueryui-ranking-subselect {padding:0px;margin:0px;display:inline-block;width:431px;font-size:12px;border-left:1px solid #bcbcbc;border-right:1px solid #bcbcbc;}
.jqueryui-ranking-subselect li {padding:0px;margin:0px;list-style-type:none;display:inline-block;width:83px;text-align:center;height:30px;line-height:30px;cursor:pointer;padding-right:0px;}
.jqueryui-ranking-subselect .ui-selectee {color:#325d64; }
.jqueryui-ranking-subselect .ui-selecting { background: #FECA40; }
.jqueryui-ranking-subselect .ui-selected { background: #7895a3; color: white; height:30px;}

/*Ranking - Diagnosis Subselect*/
.jqueryui-ranking-subselect-diagnosis {padding:0px;margin:0px;display:inline-block;width:431px;font-size:12px;border-left:1px solid #bcbcbc;border-right:1px solid #bcbcbc;}
.jqueryui-ranking-subselect-diagnosis li {padding:0px;margin:0px;list-style-type:none;display:inline-block;width:140px;text-align:center;height:30px;line-height:30px;cursor:pointer;padding-right:0px;}
.jqueryui-ranking-subselect-diagnosis .ui-selectee {color:#325d64; }
.jqueryui-ranking-subselect-diagnosis .ui-selecting { background: #FECA40; }
.jqueryui-ranking-subselect-diagnosis .ui-selected { background: #7895a3; color: white; height:30px;}

/*Ranking Display*/
ul.clinic_rank {border-bottom:1px solid #bcbcbc;margin:0px;padding:0px;display:inline-block;float:left;width:410px;margin:10px;padding-top:10px;padding-bottom:20px;margin-bottom:0px;}
ul.clinic_rank.bottom {border-bottom:0px solid #ffffff;}
ul.clinic_rank li {list-style-type:none;display:inline-block;margin:0px;padding:0px;float:left;width:410px;}
ul.clinic_rank li.clinic_name {padding-left:0px;height:25px;line-height:25px;}
ul.clinic_rank li.clinic_name a {display:block;text-decoration:none;color:#325d5d;height:25px;line-height:25px;}
ul.clinic_rank li.clinic_name a:hover {color:#103b3b;}
ul.clinic_rank li.clinic_location {color:#666666;margin-bottom:10px;margin-left:20px;font-size:11px;}
ul.clinic_rank li.clinic_stats {padding-left:20px;width:60%;font-size:12px;padding-bottom:3px;padding-top:3px;}
ul.clinic_rank li.clinic_stats span.score_type {display:inline-block;float:left;}
ul.clinic_rank li.clinic_stats span.score_result {display:inline-block;float:right;}
ul.clinic_rank li.clinic_stats span.score_result span.icon {display:inline-block;padding-left:5px;font-size:16px;}
div.clinic_rank_item {display:inline-block;width:100%;float:left;}
div.clinic_rank_item span.clinic_contact_button {float:right;position:absolute;display:inline-block;width:90px;height:80px;text-align:center;line-height:26px;border:1px solid #cccccc;background:#7895a3;color:#ffffff;font-weight:bold;margin-left:-120px;margin-top:74px;cursor:pointer;}
div.clinic_rank_item span.clinic_contact_button:hover {color:#cccccc;}
div.clinic_rank_item span.already_contacted {background:#f5f5f5;color:#7895a3;cursor:auto;float:right;position:absolute;display:inline-block;width:90px;height:80px;text-align:center;line-height:26px;border:0px solid #f5f5f5;font-weight:bold;margin-left:-120px;margin-top:74px;}
div.clinic_rank_item span.register_to_contact {float:right;position:absolute;display:inline-block;width:90px;height:80px;text-align:center;line-height:26px;border:1px solid #cccccc;background:#7895a3;color:#ffffff;font-weight:bold;margin-left:-120px;margin-top:74px;cursor:pointer;}
div.clinic_rank_item span.register_to_contact a {text-decoration:none;color:#ffffff;display:block;height:80px;}
div.clinic_rank_item span.register_to_contact a:hover {color:#cccccc;}
span.all_state_label {font-size:14px;color:#325d64;}

div.ranking_tab_content {padding:0px;margin:0px;width:100%;display:inline-block;}
h2.age_ranking_title {padding:0px;margin:0px;width:431px;float:left;display:inline-block;text-align:center;background:#7895a3;border:1px solid #bcbcbc;border-top:0px solid #7895a3;color:#ffffff;height:30px;line-height:30px;border-bottom:0px solid #7895a3;}
h2.age_ranking_title.hidden {display:none;}
div.ranking_results {background:#f5f5f5;min-height:500px;display:inline-block;width:431px;border:1px solid #bcbcbc;border-top:0px solid #e5e5e5;}
div.ranking_results div.clinic_rank_list {display:inline-block;width:100%;min-height:500px;}

/*Ranking Nav*/
ul.ranking_nav {width:100%;padding:0px;margin:0px;height:30px;float:left;display:inline-block;background:#7895a3;}
ul.ranking_nav li {list-style-type:none;padding:0px;margin:0px;height:30px;display:inline-block;width:10%;color:#ffffff;font-size:20px;line-height:30px;padding-left:15px;padding-right:15px;cursor:pointer;}
ul.ranking_nav li span.icon {cursor:pointer;}
ul.ranking_nav li:hover {color:#cccccc;}
ul.ranking_nav li.left_button {float:left;text-align:left;}
ul.ranking_nav li.right_button {float:right;text-align:right;}





/*contact_info*/
ul.contact_info {margin:0px;padding:0px;display:inline-block;width:100%;margin-top:20px;}
ul.contact_info li {margin:0px;padding:0px;display:inline-block;width:100%;list-style-type:none;margin-bottom:5px;}

/*page-body-content*/
section.page-body-content {}
div.body_content {background:url(" <%= asset_path 'contentwrapper.png' %> ") repeat-y center;padding-top:10px;}
.middle_content {padding-left:3%;padding-right:3%;display:inline-block;width:94%;float:left;}
.middle_content h1 {text-align:left;font-weight:normal;font-family:'Trebuchet MS', Arial, sans-serif;color:#00345c;font-size:20px;margin-bottom:10px;}
.middle_content p {text-align:justify;margin-bottom:15px;font-size:14px;}
.middle_content h2 {font-family:'Trebuchet MS', Arial, sans-serif;}
.middle_content hr {width:95%;margin-top:20px;margin-bottom:20px;border:0px solid #0F5151;background:#0F5151;height:1px;margin-left:auto;margin-right:auto;opacity:0.3;}

/*Middle Content*/
div.middle_content {border-left:1px solid #cccccc;border-right:1px solid #cccccc;min-height:500px;margin-top:10px;margin-bottom:10px;}

/*sidebars*/
div.left_sidebar{margin-top:10px;padding-left:12px;}
div.right_sidebar {margin-top:10px;padding-right:12px;}
ul.sidebar_module {display:inline-block;margin:0px;padding:0px;width:100%;border:1px solid #7895a3;padding:1px;margin-bottom:10px;}
ul.sidebar_module li {list-style-type:none;display:inline-block;float:left;width:192px;margin-right:5px;margin-left:5px;padding-left:5px;height:30px;line-height:30px;margin-right:10px;border-bottom:1px solid #7895a3;overflow:hidden;}
ul.sidebar_module li a {text-decoration:none;color:#325d64;display:block;height:30px;font-size:12px;}
ul.sidebar_module li.social_media {height:auto;text-align:center;}
ul.sidebar_module li.state {border-bottom:0px solid #ffffff;}
ul.sidebar_module li.state span.state_span {display:inline-block;width:50%;float:left;color:#325d64;font-size:14px;}
ul.sidebar_module li.state span.state_span input {float:right;margin-right:40px;margin-top:9px;}
div.facebookbadge {border:1px solid #7895a3;margin:10px;}
div.twitterbadge {border:1px solid #7895a3;margin:10px;padding-top:20px;padding-bottom:20px;}
ul.sidebar_module li.bottom {border:0px;}
ul.sidebar_module li.header {margin-left:0px;padding-left:5px;width:202px;background:#7895a3;color:#ffffff;font-weight:bold;height:40px;line-height:40px;}
ul.sidebar_module div.clinics_contacted{display:inline-block;width:100%;float:left;max-height:420px;overflow:scroll;}
/*footer css */
div.footer_border {border:1px solid #cccccc;display:inline-block;width:100%;background:#ffffff;}
div.footer_border ul {display:inline-block;width:100%;padding:0px;margin:0px;float:left;height:40px;}
div.footer_border ul li {list-style-type:none;float:left;margin:0px;padding:0px;display:inline-block;height:40px;line-height:40px;width:20%;text-align:center;}
div.footer_border ul li a {text-decoration:none;display:block;height:40px;color:#7895a3;font-size:13px;}
div.footer_text {font-size:13px;color:#7895a3;text-align:center;height:30px;line-height:30px;}
div.footer_text a {color:#325d64;}

/*IVFr Gradients*/
dark-green-gradient{
	background: rgb(50,93,100); /* Old browsers */
	background: -moz-linear-gradient(top, rgba(50,93,100,1) 0%, rgba(120,149,163,1) 80%, rgba(120,149,163,1) 99%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(50,93,100,1)), color-stop(80%,rgba(120,149,163,1)), color-stop(99%,rgba(120,149,163,1))); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top, rgba(50,93,100,1) 0%,rgba(120,149,163,1) 80%,rgba(120,149,163,1) 99%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top, rgba(50,93,100,1) 0%,rgba(120,149,163,1) 80%,rgba(120,149,163,1) 99%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top, rgba(50,93,100,1) 0%,rgba(120,149,163,1) 80%,rgba(120,149,163,1) 99%); /* IE10+ */
	background: linear-gradient(top, rgba(50,93,100,1) 0%,rgba(120,149,163,1) 80%,rgba(120,149,163,1) 99%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#325d64', endColorstr='#7895a3',GradientType=0 ); /* IE6-9 */
}


/* round stuff */
.round {-moz-border-radius:10px;-webkit-border-radius:10px;border-radius:10px;}
.small-round {-moz-border-radius:4px;-webkit-border-radius:4px;border-radius:4px;}
.medium-round {-moz-border-radius:15px;-webkit-border-radius:15px;border-radius:15px;}
.rounder {-moz-border-radius:20px;-webkit-border-radius:20px;border-radius:20px;}
.bottom-round {-moz-border-radius-bottomright:10px;-moz-border-radius-bottomleft:10px;border-bottom-left-radius:10px;border-bottom-right-radius:10px;-webkit-border-bottom-left-radius:10px;-webkit-border-bottom-right-radius:10px;}
.top-round {-moz-border-radius-topright:10px;-moz-border-radius-topleft:10px;border-top-left-radius:10px;border-top-right-radius:10px;-webkit-border-top-left-radius:10px;-webkit-border-top-right-radius:10px;}
.bottom-left-round {-moz-border-radius-bottomleft:10px;border-bottom-left-radius:10px;-webkit-border-bottom-left-radius:10px;}
.bottom-right-round {-moz-border-radius-bottomright:10px;border-bottom-right-radius:10px;-webkit-border-bottom-right-radius:10px;}
.left-round {-moz-border-radius-bottomleft:10px;border-bottom-left-radius:10px;-webkit-border-bottom-left-radius:10px;-moz-border-radius-topleft:10px;border-top-left-radius:10px;-webkit-border-top-left-radius:10px;}
.round_except_top_left {-webkit-border-top-right-radius:10px;border-top-right-radius:10px;-moz-border-radius-topright:10px;-moz-border-radius-bottomright:10px;-moz-border-radius-bottomleft:10px;border-bottom-left-radius:10px;border-bottom-right-radius:10px;-webkit-border-bottom-left-radius:10px;-webkit-border-bottom-right-radius:10px;}
.round_except_bottom_left {-moz-border-radius-bottomright:10px;border-bottom-right-radius:10px;-webkit-border-bottom-right-radius:10px;-moz-border-radius-topright:10px;-moz-border-radius-topleft:10px;border-top-left-radius:10px;border-top-right-radius:10px;-webkit-border-top-left-radius:10px;-webkit-border-top-right-radius:10px;}

/*AJAX*/
.ajax_loading {height:400px;background:url("<%= asset_path 'ajax-loader.gif' %>") no-repeat center center;}

/*Review Writing*/
div.review_dark_bg {position:fixed;left:0;right:0;top:0;bottom:0;background:#325d46;z-index:50;opacity:0.6;}
span.close_button {display:inline-block;width:60px;height:60px;font-size:50px;line-height:60px;text-align:center;right:0;top:0;float:right;color:#ffffff;cursor:pointer;}
span.close_button:hover {color:#bbbbbb;}
div.review_form {position:fixed;width:436px;margin-left:auto;margin-right:auto;z-index:100;background:#7895a3;top:10%;border:1px solid #cccccc;color:#ffffff;padding:8px;bottom:10%;}
div.review_form h2 {border-bottom:1px solid #cccccc;font-size:18px;}
div.review_form ul.star_rating {list-style:none;width:100%;display:inline-block;float:left;}
div.review_form ul.star_rating li {list-style-type:none;margin:0px;padding:0px;width:100%;float:left;height:40px;line-height:40px;margin-top:6px;margin-bottom:6px;}
div.review_form ul.star_rating li.review_space {height:300px;}
div.review_form ul.star_rating li span.star_label {float:left;display:inline-block;}
div.review_form ul.star_rating li span.stars {float:right;display:inline-block;font-size:24px;}
div.review_form ul.star_rating li span.stars input {width:300px;height:30px;}
div.review_form ul.star_rating li span.stars textarea {width:300px;height:200px;}
div.review_form ul.star_rating li span.stars span.icon {display:inline-block;cursor:pointer;}
div.review_form ul.star_rating li span.stars span.icon.yellow {color:#EEC900;}
div.review_form ul.star_rating li span.stars span.icon.permyellow {color:#ffd900;}
div.write_button {display:inline-block;position:relative;margin-top:-44px;float:right;}
span.write_a_review {text-decoration:underline;color:#325d42;cursor:pointer;}

/*New Sessions (signin)*/
div.sign_in_form {}
div.sign_in_form form {margin:24px;border:1px solid #7895a3;padding:24px;}
div.sign_in_form form h3 {color:#325d5d;margin-top:0px;border-top:0px;font-size:16px;margin-bottom:36px;}
div.sign_in_form div.field {display:inline-block;width:100%;height:40px;line-height:40px;}
div.sign_in_form div.field label {float:left;display:inline-block;line-height:30px;}
div.sign_in_form div.field input {float:right;display:inline-block;line-height:20px;font-size:14px;width:50%;}
div.sign_in_form input.sign-in-button {font-size:18px;}

/*Hidden*/
.hidden {display:none;}