div.user_profile h2 {font-family:'Trebuchet MS', Arial, sans-serif;color:#00345c;margin-top:30px;margin-bottom:10px;}
div.user_profile hr {margin-bottom:10px;margin-top:10px;}

/*User New (Register) Page*/
ul.content_list {width:100%;margin-bottom:20px;}
ul.content_list li {margin-left:20px;font-size:14px;width:100%;margin-bottom:5px;}
h2.register_blurb {color:#00345c;font-size:16px;text-align:center;}
table.register_form {display:inline-block;width:90%;text-align:center;border:1px solid #C2CFD6;margin-left:5%;margin-right:5%;padding-top:10%;padding-bottom:5%;}
table.register_form tr {width:100%;}
table.register_form tr td {width:55%;}
table.register_form tr td.label {text-align:right;font-size:14px;padding-right:10px;width:45%;}
table.register_form tr td.submit {padding-top:20px;}

/*User Show (Profile) Page*/
div.profile_top {display:inline-block;width:100%;float:left;height:70px;}
div.gravatar-and-name {display:inline-block;width:50%;float:left;}
div.gravatar-and-name img{border:4px solid #7895a3;float:left;}
div.gravatar-and-name div.user_info {width:70%;display:inline-block;float:left;}
div.gravatar-and-name div.user_info span.user_subtitle {margin-left:10px;font-size:10px;color:#325d64;font-family: Arial, sans-serif;}
div.gravatar-and-name div.user_info span.user_subtitle a {color:#325d64;text-decoration:none;}
div.gravatar-and-name div.user_info span.user_subtitle a:hover {color:#7895a3;}
div.gravatar-and-name div.user_info h1 {width:100%;float:left;display:inline-block;margin-top:10px;margin-left:10px;margin-bottom:0px;}
div.edit-profile-link {display:inline-block;width:30%;float:right;margin-left:20%;font-size:14px;padding-top:15px;}
div.edit-profile-link a {color:#325d64;text-decoration:none;}
div.edit-profile-link a:hover {color:#7895a3;}

div.user_info_section {display:inline-block;width:100%;float:left;}
div.user_info_section table {width:90%;margin-left:10%;font-size:14px;}
div.user_info_section table tr {height:30px;line-height:30px;}
div.user_info_section table tr td {}
div.user_info_section table tr td.label {}
div.user_info_section table tr td.data {}

div.user_info_section span.new_cycle_button {cursor:pointer;font-size:12px;color:#325d64;padding:0px;padding-left:4%;display:inline-block;width:80%;margin-bottom:20px;}
div.user_info_section span.new_cycle_button:hover {color:#7895a3;}
div.user_info_section ul.cycle_list {display:inline-block;width:100%;float:left;padding:0px;margin:0px;list-style-type:none;}
div.user_info_section ul.cycle_list li {display:inline-block;width:90%;float:left;padding:0px;margin:0px;list-style-type:none;margin-left:4%;margin-right:4%;background:#89a6b4;height:50px;margin-bottom:10px;border:1px solid #678492;line-height:30px;}
div.user_info_section ul.cycle_list li span.attempt_number {float:left;color:#102b42;font-weight:bold;line-height:50px;display:inline-block;width:5%;margin-left:10px;}
div.user_info_section ul.cycle_list li span.clinic_name {float:left;color:#102b42;font-weight:bold;display:inline-block;padding-left:10px;width:50%;}
div.user_info_section ul.cycle_list li span.clinic_info {float:left;color:#213c53;display:inline-block;width:70%;font-size:13px;padding-left:20px;line-height:10px;}
div.user_info_section ul.cycle_list li span.delete_option {cursor:pointer;float:right;color:#102b42;line-height:50px;font-size:18px;display:inline-block;padding-right:10px;font-weight:normal;} 
div.user_info_section ul.cycle_list li span.delete_option:hover {color:#325d64;}
div.user_info_section div.update_button {display:inline-block;text-align:center;float:left;margin-left:35%;margin-right:35%;width:30%;margin-top:10px;}

/*User Edit (Settings) Page*/