/*index styles for map etc*/
div.map_fade_screen {position:fixed;background:#325d64;opacity:0.5;left:0px;right:0px;top:0px;bottom:0px;z-index:3;}
#clinic_pop_up {position:absolute;width:100%;min-height:100%;left:0px;right:0px;top:0px;bottom:0px;z-index:4;margin-bottom:30px;}
#close_button {position:fixed;right:10px;top:2px;font-size:30px;color:#d5d5d5;}
#close_button:hover {color:#325d64;cursor:pointer;}
div.clinics_div{min-width:960px;}
ul.clinic_list {border:2px double #325d64;background:#7895a3;padding:0px;margin:0px;display:inline-block;width:70%;margin-left:15%;margin-right:15%;margin-top:400px;min-height:530px;}
ul.clinic_list li {padding:0px;margin:0px;display:inline-block;list-style-type:none;width:80%;height:30px;line-height:30px;font-size:15px;padding-left:10%;padding-right:10%;}
ul.clinic_list li.title {color:#ffffff;font-size:20px;font-weight:bold;text-align:center;margin-top:10px;margin-bottom:20px;}
ul.clinic_list li a.clinic_link{text-decoration:none;color:#ffffff;float:left;}
ul.clinic_list li a.clinic_link:hover{font-weight:bold;}
ul.clinic_list li span.clinic_info{float:right;color:#ffffff;}


/*clinic show page styles*/
ul.ranking_tabs {height:32px;}
ul.ranking_tabs li  {height:30px;text-align:center;width:15%;}
ul.ranking_tabs li a {height:30px;line-height:30px;text-align:center;display:block;width:100%;}
div.tab_content {padding:10px;margin-top:34px;}

.middle_content h2 {font-family:'Trebuchet MS', Arial, sans-serif;font-size:17px;margin-top:20px;margin-bottom:20px;}
ul.infobox_choices {display:inline-block;margin:0px;padding:0px;width:100%;float:left;height:30px;}
ul.infobox_choices li {display:inline-block;float:left;margin:0px;padding:0px;width:22%;text-align:center;height:30px;line-height:30px;background:#7895A3;color:#ffffff;border:1px solid #ffffff;}
ul.infobox_choices li:hover{cursor:pointer;}
ul.infobox_choices li.selected {color:#325d64;background:#B6d1de;}
div.infobox_content {border:1px solid #7895A3;float:left;margin-left:1px;margin-right:1px;width:100%;min-height:200px;}
div.infobox_content div.infobox {padding:10px;height:360px;overflow:scroll;}
div.infobox_content div.infobox p {padding:5px;}
div.infobox_content div.infobox h3 {font-size:17px;color:#325d64;margin-bottom:20px;margin-top:10px;font-weight:bold;border-bottom:1px dashed #c5c5c5;}
div.infobox_content div.infobox h4 {font-size:16px;color:#325d64;margin-bottom:10px;margin-top:10px;}
div.single_review {}
div.single_review span {display:inline-block;float:left;}
div.single_review span.review_title {height:20px;line-height:20px;font-size:18px;color:#325d5d;width:70%;overflow:hidden;font-weight:bold;margin-top:24px;}
div.single_review span.review_stars {text-align:right;float:right;height:20px;width:30%;height:20px;line-height:20px;margin-top:24px;}
div.single_review span.review_stars span.icon {display:inline-block;float:right;color:#cccccc;}
div.single_review span.review_stars span.icon.permyellow {color:#ffd900;}
div.single_review span.review_author {width:100%;font-size:13px;color:#ababab;}
div.single_review span.review_author a{font-size:13px;color:#7895a3;text-decoration:none;}
div.single_review span.review_date {text-align:left;width:100%;color:#ababab;font-size:14px;float:left;margin-top:12px;}
div.single_review div.review_content {text-align:justify;font-size:13px;color:#555555;margin-top:24px;margin-left:6px;display:inline-block;width:100%;padding-bottom:24px;border-bottom:1px solid #cccccc;padding-right:6px;}
	

#select-age {padding:0px;margin:0px;display:inline-block;width:100%;font-size:13px;margin-bottom:20px;}
#select-age li {padding:0px;margin:0px;list-style-type:none;display:inline-block;width:18%;text-align:center;height:25px;line-height:25px;cursor:pointer;margin-top:5px;}
#select-age .ui-selectee {background:#7895a3;color:#ffffff;border:1px solid #325d5d; }
#select-age .ui-selecting { background: #FECA40; }
#select-age .ui-selected { background: #F39814; color: white; }
div.legend {font-size:10px;text-align:center;margin-bottom:30px;}

table.score_table {width:100%;}
table.score_table tr {border-bottom:1px solid #ccccff;height:40px;line-height:40px;}
table.score_table tr td {}
table.score_table tr td.score_label {text-align:left;font-weight:bold;color:#325d5d;}
table.score_table tr td.score_result {text-align:right;}
table.score_table tr td.score_result span {font-size:18px;font-weight:bold;line-height:30px;}
span.green {color:#00CD00;}
span.blue {color:#0000CD;}
span.yellow {color:#FFE600;}
span.gray {color:#999999;}


/*global stuff*/
div.hidden {display:none;}