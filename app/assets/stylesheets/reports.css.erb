/* Colors: Mid: #2F7C7C Mid-Dark:#325D5D Dark:#0F5151 Mid-Light:#65BDBD Light:#7DBDBD IVFR:#7895a3
/* The IVF Process */
div.faqquestion {background:#7895a3;color:#ffffff;cursor:pointer;padding:5px;margin-bottom:10px;}

/* Fertility News */
div.rss_title {margin-top:20px;margin-bottom:10px;}
div.rss_title a {text-decoration:none;color:#7895a3;}
div.rss_byline{font-size:12px;color:#555555;text-align:right;margin-bottom:10px;font-style:italic;}
p.rss_feed {border-bottom:1px solid #d5d5d5;padding-bottom:10px;}

.hidden {display:none;}

/*Clinic Explorer*/
.explorer_content h1 {text-align:center;font-weight:normal;font-family:'Trebuchet MS', Arial, sans-serif;color:#00345c;font-size:20px;margin-bottom:10px;}
.explorer_center {min-height:950px;}

#age-selectable {padding:0px;margin:0px;display:inline-block;width:80%;font-size:13px;margin-top:20px;margin-bottom:20px;padding-left:10px;padding-right:10px;}
#age-selectable li {padding:0px;margin:0px;list-style-type:none;display:inline-block;width:100%;text-align:center;height:25px;line-height:25px;cursor:pointer;margin-top:5px;}
#age-selectable .ui-selectee {background:#7895a3;color:#ffffff;border:1px solid #325d5d; }
#age-selectable .ui-selecting { background: #FECA40; }
#age-selectable .ui-selected { background: #F39814; color: white; }

#diagnosis-selectable {padding:0px;margin:0px;display:inline-block;width:80%;font-size:13px;margin-top:20px;margin-bottom:20px;padding-left:10px;padding-right:10px;}
#diagnosis-selectable li {padding:0px;margin:0px;list-style-type:none;display:inline-block;width:100%;text-align:center;height:25px;line-height:25px;cursor:pointer;margin-top:5px;}
#diagnosis-selectable .ui-selectee {background:#7895a3;color:#ffffff;border:1px solid #325d5d; }
#diagnosis-selectable .ui-selecting { background: #FECA40; }
#diagnosis-selectable .ui-selected { background: #F39814; color: white; }

#year-selectable {padding:0px;margin:0px;display:inline-block;width:80%;font-size:13px;margin-top:20px;margin-bottom:20px;padding-left:10px;padding-right:10px;}
#year-selectable li {padding:0px;margin:0px;list-style-type:none;display:inline-block;width:100%;text-align:center;height:25px;line-height:25px;cursor:pointer;margin-top:5px;}
#year-selectable .ui-selectee {background:#7895a3;color:#ffffff;border:1px solid #325d5d; }
#year-selectable .ui-selecting { background: #FECA40; }
#year-selectable .ui-selected { background: #F39814; color: white; }

li#cycles-value {text-align:left;margin-bottom:6px;font-weight:bold;}

ul.parameter {border:1px solid #d5d5d5;padding:0px;margin:0px;display:inline-block;width:90%;font-size:13px;padding-left:5%;padding-right:5%;margin-top:10px;}
ul.parameter li {list-style-type:none;padding:0px;margin:0px;display:inline-block;height:20px;width:100%;}
ul.parameter li.label {font-weight:bold;font-size:14px;color:#325d5d;margin-bottom:5px;}
ul.parameter li.value {color:#7895a3;text-align:center;}
ul.parameter li.value input.input-value {color:#7895a3;border:0px;width:35px;text-align:center;font-size:12px;}
ul.parameter li.slider {}
	
div#clinic_list ul {display:inline-block;width:90%;padding:0px;margin:0px;padding-left:2%;padding-right:8%;background:#C2CFD6;border:1px solid #7895a3;color:#0F5151;margin-bottom:10px;}
div#clinic_list ul li {overflow:hidden;display:inline-block;width:100%;padding:0px;margin:0px;list-style-type:none;height:25px;font-size:14px;line-height:25px;}
div#clinic_list ul li.clinic_name {}
div#clinic_list ul li.clinic_name a {text-decoration:none;color:#0F5151;font-weight:bold;}
div#clinic_list ul li.clinic_stats {}